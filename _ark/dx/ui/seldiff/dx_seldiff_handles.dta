            (guitar {set $random_diff {{song_select_panel find "guitar.idd"} get difficulty}})
            (real_guitar {set $random_diff {{song_select_panel find "guitar_pro.idd"} get difficulty}})
            (bass {set $random_diff {{song_select_panel find "bass.idd"} get difficulty}})
            (real_bass {set $random_diff {{song_select_panel find "bass_pro.idd"} get difficulty}})
            ((drum real_drum) {set $random_diff {{song_select_panel find "drum.idd"} get difficulty}})
            (keys {set $random_diff {{song_select_panel find "keys.idd"} get difficulty}})
            (real_keys {set $random_diff {{song_select_panel find "keys_pro.idd"} get difficulty}})
            (vocals {set $random_diff {{song_select_panel find "vocals.idd"} get difficulty}})
         }
         {set $random_artist FALSE}
         {song_select_panel dx_authorpoll $random_artist_song}
         {set $random_artist $artistvar}
         {set $random_album FALSE}
         {set $random_arist_album FALSE}
         {song_select_panel dx_authorpoll $random_album_song}
         {set $random_album $albumvar}
         {set $random_arist_album $artistvar}
         {set $random_ugc_source FALSE}
         {set $random_ugc_author FALSE}
         {song_select_panel dx_authorpoll $random_ugc_song}
         {switch $random_ugc_song
            DX_SOURCE_SWITCH
