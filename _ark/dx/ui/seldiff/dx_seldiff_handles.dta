   (dx_refesh_songs
      ;sort by difficulty so we have a super small chance of hitting headers
      {set $dx_pas_sort_changed TRUE}
      {song_select_filter_panel filter_enter}
      {{music_library get view_settings_provider} select_setting 1}
      {{music_library get view_settings_provider} select_setting_option 3}
      {music_library report_sort_and_filters}
      {song_select_filter_panel filter_exit}
      ;we actually roll for songs 30 times for each line
      ;boy i hope we find one with the instrument you use
      {set $numtimes ()}
      {resize $numtimes 30}

      {set $play_a_show_found_genre_part FALSE}
      {set $play_a_show_found_year_part FALSE}
      {set $play_a_show_found_length_part FALSE}
      {set $play_a_show_found_song_part FALSE}
      {set $play_a_show_found_tempo_part FALSE}
      {set $play_a_show_found_album_part FALSE}
      {set $play_a_show_found_artist_part FALSE}
      {set $play_a_show_found_diff_part FALSE}
      {set $play_a_show_found_ugc_part FALSE}
