               {ui goto_screen campaign_leaderboard_hub_screen}
            }
            {set [waiting_for_leaderboard_entry] FALSE}
         )
      }
   )
   (unlock_gamestop_award
      ($pad_num)
      {if {! {ui in_transition}}
         {do
            ($user {user_mgr get_user_from_pad_num $pad_num})
            ($profile {profile_mgr get_profile $user})
